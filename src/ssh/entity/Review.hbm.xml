<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping> 
	<class name="ssh.entity.Review" table="Review">
    	<id name="id" column="id">
    		 <generator class="identity" />
    	</id>
    	<property name="ids" column="ids"></property>
    	<property name="name" column="name"></property>
    	<property name="message" column="message"></property>
    	<property name="yaoqiu" column="yaoqiu"></property>
    	<property name="type_id" column="type_id"></property>
    </class>
</hibernate-mapping>