<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping> 
	<class name="ssh.entity.Welfare" table="Welfare">
    	<id name="id" column="id">
    		 <generator class="identity" />
    	</id>
    	<property name="ids" column="ids"></property>
    	<property name="name" column="name"></property>
    	<property name="type" column="type"></property>
    	<property name="date" column="date"></property>
    	<property name="time" column="time"></property>
    	<property name="welfare_way" column="welfare_way"></property>
    	<property name="welfare_people" column="welfare_people"></property>
    	<property name="welfare_money" column="welfare_money"></property>
    	<property name="effective" column="effective"></property>
    	<property name="endtime" column="endtime"></property>
    </class>
</hibernate-mapping>