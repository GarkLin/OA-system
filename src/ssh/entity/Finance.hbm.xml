<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping> 
	<class name="ssh.entity.Finance" table="Finance">
    	<id name="finance_id" column="finance_id">
    		 <generator class="identity" />
    	</id>
    	<property name="finance_type" column="finance_type"></property>
    	<property name="finance_name" column="finance_name"></property>
    	<property name="finance_department" column="finance_department"></property>
    	<property name="finance_money" column="finance_money"></property>
    	<property name="finance_date" column="finance_date"></property>
    	<property name="fid" column="fid"></property>
    </class>
</hibernate-mapping>